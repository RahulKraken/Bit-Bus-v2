<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.NextBusFragment">

    <fragment
        android:id="@+id/nextStudentBusFragment"
        android:name="com.example.kraken.bit_bus_v2.Fragments.NextStudentBusFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="350dp"
        android:layout_height="380dp" />

    <fragment
        android:id="@+id/nextStaffBusFragment"
        android:name="com.example.kraken.bit_bus_v2.Fragments.NextStaffBusFragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="280dp"
        android:layout_height="300dp" />

</android.support.constraint.ConstraintLayout>